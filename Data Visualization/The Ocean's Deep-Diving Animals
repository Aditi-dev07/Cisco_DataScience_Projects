          "data": {
            "text/plain": [
              "<Figure size 600x300 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "plt.figure(figsize=(6, 3))\n",
        "plt.barh(df['category'], df['max_depth'])\n",
        "plt.xlabel('Maximum Depth (meters)')\n",
        "\n",
        "# Pro Tip 3\n",
        "ax = plt.gca()\n",
        "ax.spines[['top','bottom','left','right']].set_visible(False)\n",
        "\n",
        "plt.show()"
      ]
    },
