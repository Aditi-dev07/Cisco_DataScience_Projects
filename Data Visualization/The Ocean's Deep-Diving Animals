        "filtered_combined_df = combined_df[\n",
        "    (combined_df['category'] != 'reference') |\n",
        "    (combined_df['animal'] == 'Burj Khalifa (height)') |\n",
        "    (combined_df['animal'] == 'Titanic Wreck')\n",
        "].copy()\n",
        "\n",
        "plt.figure(figsize=(10, 8))\n",
        "plt.barh(filtered_combined_df['animal'], filtered_combined_df['depth'], color=filtered_combined_df['plot_color'])\n",
        "plt.xlabel('Depth (meters)')\n",
        "plt.title('Midnight Zone Divers, Echolocation, and Selected Reference Depths')\n",
        "clean_bar_axes()\n",
        "\n",
        "# Add a legend for clarity\n",
        "echo_patch = plt.matplotlib.patches.Patch(color='C2', label='Echolocating Species')\n",
        "no_echo_patch = plt.matplotlib.patches.Patch(color='C0', label='Non-Echolocating Species')\n",
        "reference_patch = plt.matplotlib.patches.Patch(color='C1', label='Selected Reference Points')\n",
        "plt.legend(handles=[echo_patch, no_echo_patch, reference_patch], loc='lower right')\n",
